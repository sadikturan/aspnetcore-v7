@model List<Course>

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>course list</h1>

    @foreach(var kurs in Model) 
    {
        <div id="kurs_@kurs.Id">
            <h2>@kurs.Title</h2>
            <p>@kurs.Description</p>
            <img src="~/img/@kurs.Image" width="200" alt="">
        </div>
    }

</body>
</html>